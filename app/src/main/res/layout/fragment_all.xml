<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="allFragmentViewModel"
            type="com.example.leafapp.homefragments.allFragment.AllFragmentViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".homefragments.allFragment.AllFragment">

        <!--            listData="@{allFragmentViewModel.ll}"-->

        <androidx.recyclerview.widget.RecyclerView

            android:id="@+id/all_RC"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"

            tools:listitem="@layout/post_card" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>